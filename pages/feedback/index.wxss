.feedback {
    padding: 40rpx 30rpx;
}

.feedback image {
    width: 400rpx;
    display: flex;
    margin: 40rpx auto;
}

.h3 {
    font-weight: bold;
    font-size: 40rpx;
    text-align: center;
}

.btn {
    width: 400rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 40rpx;
    color: #fff;
    background-color: #09f;
    text-align: center;
    margin: 40rpx auto;
}
